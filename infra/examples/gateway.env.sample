# Copy to compose/gateway.env and edit values before running docker compose.
KM_AUTH_ENABLED=true
KM_READER_TOKEN=replace-with-reader-token
KM_ADMIN_TOKEN=replace-with-maintainer-token
KM_NEO4J_PASSWORD=replace-with-strong-password
KM_NEO4J_AUTH_ENABLED=true
KM_CONTENT_ROOT=/workspace/repo
KM_CONTENT_DOCS_SUBDIR=docs
KM_INGEST_USE_DUMMY=true
KM_QDRANT_URL=http://qdrant:6333
KM_NEO4J_URI=bolt://neo4j:7687
KM_SEED_SAMPLE_REPO=false
KM_ALLOW_INSECURE_BOOT=false
